{
    "name": "biznisbox/biznisbox",
    "type": "project",
    "version": "2.0.0-dev",
    "description": "BiznisBox is a free and open-source business management system for small and medium-sized businesses.",
    "keywords": [
        "laravel",
        "invoice",
        "crm",
        "erp",
        "accounting",
        "business",
        "management",
        "system",
        "software",
        "saas",
        "open-source",
        "free",
        "self-hosted"
    ],
    "license": "AGPL-3.0-or-later",
    "homepage": "https://www.biznisbox.com",
    "php-ext": {
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-zip": "*"
    },
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-dompdf": "^3.0",
        "hisorange/browser-detect": "^5.0",
        "laravel/framework": "^11.15",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "laravolt/avatar": "^5.1",
        "league/omnipay": "^3.2",
        "milon/barcode": "^11.0",
        "nnjeim/world": "^1.1",
        "nordigen/nordigen-php": "^1.1",
        "omnipay/paypal": "^3.0",
        "owen-it/laravel-auditing": "^13.6",
        "pragmarx/google2fa": "^8.0",
        "spatie/laravel-permission": "^6.9",
        "spatie/laravel-webhook-server": "^3.8",
        "stripe/stripe-php": "^15.0",
        "tymon/jwt-auth": "^2.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.1",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app\\Utils\\Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
